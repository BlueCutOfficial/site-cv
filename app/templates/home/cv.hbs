<div class="cv aligncenter">
  <div>
    <button class="borderbox roundedbox"
      type="button"
      {{on "click" this.onSwitch}}>
      {{this.selected.text}}
    </button>
  </div>
  <object class="cv-object"
    width="650"
    height="960"
    type="application/pdf"
    data="{{this.selected.file}}">
  </object>
</div>
